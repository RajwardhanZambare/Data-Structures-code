class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        int j=0;
        for(int i=m; i<nums1.size(); i++){//int i=m means start from the extra zeroes in the nums1 vector. this will just replace the extra zeroes in the nums1 vector instead of zeroes before the extra zeroes
            if(nums1[i]==0){
                nums1[i]=nums2[j];
                j++;
            }
        }
        for(int i=0; i<nums1.size(); i++){
            for(int j=0; j<nums1.size(); j++){
                if(nums1[i]<nums1[j]){
                    swap(nums1[i],nums1[j]);
                }
            }
        }
    }
};
