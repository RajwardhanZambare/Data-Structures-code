//cpp
class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int ans=0;
        for(int values: nums){
            ans=ans^values;   //X-OR operator is used which gives 1 when both are not same
        }
        return ans;
    }
};
//java
class Solution {
    public int singleNumber(int[] nums) {
        int nonRepeatNum = nums[0];
        for(int i=1; i<nums.length; i++){
            nonRepeatNum = nonRepeatNum ^ nums[i];
        }
        return nonRepeatNum;
    }
}
